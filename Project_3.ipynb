    "#Code to count frequency of a character\n",
    "def most_frequent(string):\n",
    "    a = {}\n",
    "    for i in string:\n",
    "        if i in a:\n",
    "            a[i] += 1\n",
    "        else:\n",
    "            a[i] = 1\n",
    "    return a\n",
    "string = input(\"Enter a string :\")\n",
    "most_frequent(string)\n"
   ]
